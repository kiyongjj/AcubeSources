<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
	<servlet>
		<servlet-name>action</servlet-name>
		<servlet-class>com.sds.rqreport.service.web.RQGetfileversion</servlet-class>
	</servlet>
  	<servlet>
    	<servlet-name>SetScheduleStatus</servlet-name>
    	<servlet-class>com.sds.rqreport.service.web.SetScheduleStatus</servlet-class>
  	</servlet>
  	<servlet>
    	<servlet-name>rqservlet</servlet-name>
    	<servlet-class>com.sds.rqreport.service.web.RQServlet</servlet-class>
  	</servlet>
  	<servlet>
    	<servlet-name>rqdownloadservlet</servlet-name>
    	<servlet-class>com.sds.rqreport.service.web.RQServlet</servlet-class>
  	</servlet>
    <servlet>
	    <servlet-name>RQDocStat</servlet-name>
	    <servlet-class>com.sds.rqreport.service.web.RQDocStat</servlet-class>
	</servlet>

  	
	<servlet-mapping>
		<servlet-name>action</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>
  	<servlet-mapping>
    	<servlet-name>SetScheduleStatus</servlet-name>
    	<url-pattern>/servlet/SetScheduleStatus.do</url-pattern>
  	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>rqservlet</servlet-name>
	    <url-pattern>/rqservlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>rqdownloadservlet</servlet-name>
	    <url-pattern>/rqdownloadservlet.do</url-pattern>
	</servlet-mapping>
  <servlet-mapping>
    <servlet-name>RQDocStat</servlet-name>
    <url-pattern>/RQDocStat.do</url-pattern>
  </servlet-mapping>
	
	<session-config>
		<session-timeout>60</session-timeout>
	</session-config>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<resource-ref>
		<description>Connection</description>
		<res-ref-name>jdbc/Repository</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>
</web-app>
